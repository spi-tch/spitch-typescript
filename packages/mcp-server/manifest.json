{
  "dxt_version": "0.2",
  "name": "spitch-mcp",
  "version": "0.0.1",
  "description": "The official MCP Server for the Spitch API",
  "author": {
    "name": "Spitch",
    "email": "dev@spitch.app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stainless-sdks/spitch-typescript.git"
  },
  "homepage": "https://github.com/stainless-sdks/spitch-typescript/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.spitch.app",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "SPITCH_API_KEY": "${user_config.SPITCH_API_KEY}"
      }
    }
  },
  "user_config": {
    "SPITCH_API_KEY": {
      "title": "api_key",
      "description": "",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
